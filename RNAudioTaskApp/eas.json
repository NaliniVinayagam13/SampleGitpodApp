{
  "cli": {
    "version": ">= 10.0.0"
  },
  "build": {
    "bare": {
      "workflow": "generic",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "JAVA_HOME": "/usr/lib/jvm/java-17-openjdk-amd64"
      }
    }
  },
  "hooks": {
    "pre-build": "yarn prebuild-android"
  }
}
